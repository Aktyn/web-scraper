{
  "name": "web-scraper",
  "version": "3.0.21",
  "private": true,
  "type": "module",
  "author": "Rados≈Çaw Krajewski (Aktyn) <aktyn3@gmail.com>",
  "description": "A highly configurable web scraping tool",
  "license": "GPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aktyn/web-scraper.git"
  },
  "bugs": {
    "url": "https://github.com/Aktyn/web-scraper/issues"
  },
  "homepage": "https://github.com/Aktyn/web-scrapers#readme",
  "scripts": {
    "build": "turbo run build",
    "build:sea": "turbo run build && turbo run build:standalone && ./build-sea.sh",
    "dev": "turbo run dev",
    "start": "turbo run start",
    "test": "turbo run test",
    "test:coverage": "turbo run test:coverage",
    "test:watch": "turbo run test:watch",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "depcheck": "find . -name node_modules -prune -o -name package.json -execdir bash -c 'echo $(pwd): && npx depcheck ; echo -e \"\b\"' \\;"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.11",
    "prettier": "^3.6.2",
    "turbo": "^2.6.0"
  },
  "packageManager": "npm@11.6.2"
}
