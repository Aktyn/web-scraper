{
  "name": "@web-scraper/core",
  "version": "3.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "src/index.ts",
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json",
    "dev": "tsc --watch --project ./tsconfig.build.json",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit --skipLibCheck -p ./tsconfig.json",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "install:chrome": "npx rebrowser-puppeteer browsers install chrome",
    "install:firefox": "npx rebrowser-puppeteer browsers install firefox"
  },
  "dependencies": {
    "@lem0-packages/puppeteer-page-proxy": "^1.4.1",
    "@types/node-notifier": "^8.0.5",
    "@web-scraper/common": "*",
    "ghost-cursor": "^1.4.1",
    "is-plain-object": "^5.0.0",
    "kind-of": "^6.0.3",
    "node-notifier": "^10.0.1",
    "ollama": "^0.5.16",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-adblocker": "^2.13.6",
    "puppeteer-extra-plugin-portal": "^3.2.1",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "puppeteer-real-browser": "^1.4.2",
    "rebrowser-puppeteer": "^24.8.1",
    "shallow-clone": "^3.0.1",
    "sharp": "^0.34.2",
    "zod": "^3.25.67",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/node": "^22.15.32",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.0",
    "globals": "^16.2.0",
    "pptr-mock-server": "^1.5.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vitest": "^3.2.4"
  }
}
