{
  "name": "@web-scraper/core",
  "version": "3.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "src/index.ts",
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json",
    "dev": "tsc --watch --project ./tsconfig.build.json",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit --skipLibCheck -p ./tsconfig.json",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "install:chrome": "npx rebrowser-puppeteer browsers install chrome",
    "install:firefox": "npx rebrowser-puppeteer browsers install firefox"
  },
  "dependencies": {
    "@web-scraper/common": "*",
    "rebrowser-puppeteer": "^24.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/node": "^22.15.29",
    "@vitest/coverage-istanbul": "^3.2.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.1",
    "pptr-mock-server": "^1.4.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vitest": "^3.2.0"
  }
}