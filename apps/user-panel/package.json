{
  "name": "@web-scrapper/user-panel",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "cross-env npm run typecheck && react-scripts build",
    "dev": "cross-env BROWSER=none react-scripts start",
    "format": "prettier --write src/**/*{.ts,.tsx}",
    "lint": "eslint src/**/*{.ts,.tsx} --fix",
    "test": "vitest --watch=false",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage --reporter=default --reporter=junit",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "env": "shx cp ./example.env ./.env"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/roboto-flex": "^4.5.5",
    "@hookform/resolvers": "^2.9.11",
    "@mui/icons-material": "^5.11.11",
    "@mui/lab": "^5.0.0-alpha.123",
    "@mui/material": "^5.11.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "animejs": "^3.2.1",
    "deepmerge": "^4.3.0",
    "notistack": "^3.0.1",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.5",
    "react-scripts": "^5.0.1",
    "shx": "^0.3.4",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.10.0",
    "@types/animejs": "^3.1.7",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^21.1.1",
    "react-refresh": "^0.11.0",
    "vite-tsconfig-paths": "^4.0.7",
    "vitest": "^0.29.7"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "engines": {
    "node": ">=16"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "sideEffects": false
}
