{
  "name": "@web-scraper/user-panel",
  "version": "1.0.0",
  "private": true,
  "homepage": "./",
  "scripts": {
    "build": "cross-env yarn run typecheck && react-scripts build",
    "dev": "cross-env BROWSER=none react-scripts start",
    "format": "prettier --write src/**/*{.ts,.tsx}",
    "lint": "eslint src/**/*{.ts,.tsx} --fix",
    "test": "vitest --watch=false",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage --reporter=default --reporter=junit",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "env": "shx cp ./example.env ./.env"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource-variable/roboto-flex": "^5.0.8",
    "@hookform/resolvers": "^3.3.3",
    "@mui/icons-material": "^5.15.2",
    "@mui/lab": "5.0.0-alpha.158",
    "@mui/material": "^5.15.2",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "animejs": "^3.2.2",
    "deepmerge": "^4.3.1",
    "notistack": "^3.0.1",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "react-scripts": "^5.0.1",
    "react-simple-code-editor": "^0.13.1",
    "react-window": "^1.8.10",
    "shx": "^0.3.4",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.11.0",
    "@types/animejs": "^3.1.12",
    "@types/prismjs": "^1.26.3",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^0.34.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^23.0.1",
    "react-refresh": "^0.14.0",
    "vite-tsconfig-paths": "^4.2.2",
    "vitest": "^0.34.6"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "browserslist": {
    "production": [
      "last 1 electron version"
    ],
    "development": [
      "last 1 electron version"
    ]
  },
  "sideEffects": false
}